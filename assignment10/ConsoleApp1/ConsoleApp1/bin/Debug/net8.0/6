    <script>
        jQuery(function ($) {
            // Remove me control sign in button.
            $('#meControl').remove();

            // Reset domain for Locale Page link in UHF.
            let uhfLocaleUrl = $('#locale-picker-link').attr("href").split('/');
            if (uhfLocaleUrl.length >= 2) {
                let currentUrl = $(location).attr('href').split('/');
                uhfLocaleUrl[2] = currentUrl[2];
                $('#locale-picker-link').attr("href", uhfLocaleUrl.join('/'));
            }
        });
    </script>
